{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug mysql-sniffer (lo0)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/mysql-sniffer.go",
            "args": [
                "-i", "lo0",
                "-P", "3306",
                "-v"
            ],
            "env": {},
